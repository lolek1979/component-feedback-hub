{
  "name": "frontend-nis",
  "version": "0.0.1",
  "description": "Next.js",
  "private": true,
  "scripts": {
    "dev": "next dev -p 7296",
    "dev:turbo": "next dev --turbopack -p 7296",
    "dev:https": "next dev -p 7296 --experimental-https",
    "mocks": "mocks-server",
    "build": "next build && npm run docs:all",
    "start": "next start",
    "prettier:format": "prettier --write .",
    "prettier:check": "prettier --check \"**/*.{ts,tsx,json,css,scss,md}\"",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --cache",
    "lint:css": "stylelint **/*.css **/*.module.css",
    "lint:css:fix": "stylelint **/*.css **/*.module.css --fix",
    "eslint:format": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "test": "cross-env NODE_ENV=test NODE_NO_WARNINGS=1 jest test",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "generate": "pnpm plop --plopfile ./.plop/plopfile.js",
    "check": "npm run test && npm run prettier:check && npm run lint && npm run lint:css",
    "fix": "npm run prettier:format && npm run eslint:format && npm run lint:css:fix",
    "prepare": "husky",
    "pre-commit": "npm run update-component-css && lint-staged",
    "docs": "typedoc",
    "postprocess-docs": "node scripts/postprocess-docs.js",
    "docs:all": "npm run docs && npm run postprocess-docs",
    "update-component-css": "node scripts/updateComponentMdx.js"
  },
  "browserslist": {
    "production": [
      "last 3 versions",
      "not dead"
    ],
    "development": [
      "last 3 versions"
    ]
  },
  "engines": {
    "node": ">=18.20.2"
  },
  "dependencies": {
    "@azure/msal-browser": "^4.11.1",
    "@azure/msal-react": "^3.0.11",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^5.1.1",
    "@next/env": "15.5.4",
    "@storybook/addon-essentials": "^9.0.0-alpha.12",
    "@storybook/addon-interactions": "^9.0.0-alpha.10",
    "@storybook/blocks": "^9.0.0-alpha.17",
    "@storybook/manager-api": "^9.0.0-alpha.1",
    "@storybook/react": "^9.1.10",
    "@storybook/theming": "^9.0.0-alpha.1",
    "@svgr/webpack": "^8.1.0",
    "@t3-oss/env-nextjs": "0.12.0",
    "@tanstack/react-query": "^5.75.1",
    "@tanstack/react-query-devtools": "^5.75.2",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.12",
    "autoprefixer": "10.4.21",
    "axios": "^1.9.0",
    "babel-preset-current-node-syntax": "^1.1.0",
    "caniuse-lite": "^1.0.30001746",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "jwt-decode": "^4.0.0",
    "lodash": "^4.17.21",
    "next": "15.5.4",
    "next-intl": "^4.3.12",
    "nuqs": "^2.4.3",
    "radix-ui": "^1.3.4",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-hook-form": "^7.60.0",
    "react-to-pdf": "^1.0.1",
    "sonner": "^2.0.7",
    "use-debounce": "^10.0.4",
    "zod": "^3.24.0",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.27.1",
    "@eslint/js": "^9.25.1",
    "@mocks-server/main": "^4.1.0",
    "@react-pdf/renderer": "^4.3.0",
    "@storybook/addon-a11y": "^9.1.10",
    "@storybook/addon-docs": "^9.1.10",
    "@storybook/addon-links": "^9.1.10",
    "@storybook/nextjs": "^9.1.10",
    "@storybook/testing-library": "^0.2.2",
    "@tanstack/eslint-plugin-query": "^5.74.7",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/jest": "29.5.14",
    "@types/lodash": "^4.17.16",
    "@types/node": "22.15.3",
    "@types/react": "19.2.0",
    "@types/react-dom": "19.2.0",
    "@typescript-eslint/eslint-plugin": "8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@typescript-eslint/type-utils": "^8.31.1",
    "@typescript-eslint/utils": "^8.31.1",
    "babel-jest": "29.7.0",
    "cross-env": "^7.0.3",
    "eslint": "9.25.1",
    "eslint-config-next": "^15.5.4",
    "eslint-config-prettier": "10.1.2",
    "eslint-plugin-prettier": "5.2.6",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-storybook": "^9.1.10",
    "eslint-plugin-testing-library": "7.1.1",
    "husky": "^9.1.7",
    "identity-obj-proxy": "3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-junit": "^16.0.0",
    "jest-watch-typeahead": "2.2.2",
    "lint-staged": "^15.5.0",
    "plop": "4.0.1",
    "postcss": "8.5.3",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-nested": "^7.0.2",
    "postcss-preset-env": "^10.1.6",
    "prettier": "3.5.3",
    "storybook": "^9.1.10",
    "stylelint": "^16.19.1",
    "stylelint-config-css-modules": "^4.4.0",
    "stylelint-config-standard": "^38.0.0",
    "typedoc": "^0.28.3",
    "typedoc-plugin-missing-exports": "^4.0.0",
    "typedoc-plugin-react": "^0.1.0",
    "typescript": "^5.8.3"
  },
  "lint-staged": {
    "src/**/*": [
      "pnpm prettier:format",
      "pnpm eslint:format",
      "npm run test"
    ]
  },
  "overrides": {
    "eslint-plugin-storybook": {
      "typescript": "^5.8.3"
    },
    "sha.js": "2.4.12",
    "@types/react": "19.2.0",
    "@types/react-dom": "19.2.0"
  }
